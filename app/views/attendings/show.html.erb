<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @attending.name %>
  <br>
  <% @attending.students.each do |attending| %>
    <tr>
      <td><%= attending.first_name %></td>
      <td><%= link_to 'Show', attending %></td>
      <td><%= link_to 'Edit', edit_attending_path(attending) %></td>
      <td><%= link_to 'Destroy', attending, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <br>
    </tr>
  <% end %>
</p>

<%= link_to 'Edit', edit_attending_path(@attending) %> |
<%= link_to 'Back', attendings_path %>
